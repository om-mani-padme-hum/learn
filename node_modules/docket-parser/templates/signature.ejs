      <div id='<%-id%>' class='row'>
        <div class='col'>
          <h4 class='mt-2 pl-3'><%-title%> <small>[<a class='<%-linkClass%>' href='#top'>top</a>]</small></h4>
        </div>
      </div>
      <div class='pl-4'>
        <% if ( status ) { %>
        <div class='row'>
          <div class='col-auto'>
            <div class='alert mt-2 alert-<%-status.color()%>'>
              <strong><%-status.status(true)%></strong>: <%-status.description()%>
            </div>
          </div>
        </div>
        <% } %>
        <% if ( authors.length > 0 ) { %>
        <div class='row'>
          <div class='col'>
            <p>
              <small class='font-italic'>Written by: <%-authors%></small>
            </p>
          </div>
        </div>
        <% } %>
        <% if ( added || updates.length > 0 ) { %>
        <div class='row'>
          <div class='col-auto'>
            <p>
              <small>&#9656; <a class='<%-linkClass%>' data-toggle='collapse' href='#collapse<%-count%>'>History</a></small>
            </p>
            <div class='collapse' id='collapse<%-count%>'>
              <div class='card card-body p-1 px-3 ml-3 mb-2'>
        <% } %>
        <% if ( added ) { %>
                <small>Added in: <%-added.version()%> <%-added.description()%></small>
        <% } %>
        <% if ( updates.length > 0 ) { %>
        <%   updates.forEach((update) => { %>
                <small>Updated in: <%-update.version()%> <%-update.description()%></small>
        <%   }); %>
        <% } %>
        <% if ( added || updates.length > 0 ) { %>
              </div>
            </div>  
          </div>
        </div>
        <% } %>
        <% if ( params.length > 0 || throws.length > 0 || returns ) { %>
        <div class='row'>
          <div class='col'>
            <ul>
        <%   params.forEach((param) => { %>
              <li class='py-1'><code class='text-dark p-1'><%-param.name()%></code> <%-param.type()%> <%-param.description()%></li>
        <%   }); %>
        <% if ( throws.length > 0 ) { %>
        <%   throws.forEach((err) => { %>
              <li class='py-1 pl-1'>Throws <%-err.type()%> <%-err.description()%></li>
        <%   }); %>
        <% } %>
        <% if ( returns ) { %>
              <li class='py-1 pl-1'>Returns <%-returns.type()%> <%-returns.description()%></li>
        <% } %>
            </ul>
          </div>
        </div>
        <% } %>
        <% if ( description.length > 0 ) { %>
        <div class='row'>
          <div class='col'>
            <p><%-description%></p>
          </div>
        </div>
        <% } %>
      </div>
